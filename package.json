{
  "name": "pro-shop",
  "version": "1.0.0",
  "description": "MERN shopping cart app",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node BackEnd/server",
    "server": "nodemon BackEnd/server",
    "client": "npm start --prefix FrontEnd",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node BackEnd/seeder",
    "data:destroy": "node BackEnd/seeder -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/0xAureoleNode/pro-shop.git"
  },
  "author": "Aureole",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/0xAureoleNode/pro-shop/issues"
  },
  "homepage": "https://github.com/0xAureoleNode/pro-shop#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "chalk": "^5.1.2",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "mongoose": "^6.7.2"
  },
  "devDependencies": {
    "concurrently": "^7.5.0",
    "nodemon": "^2.0.20"
  }
}
